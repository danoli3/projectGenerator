{
  "name": "project-generator",
  "productName": "projectGenerator",
  "version": "0.2.0",
  "description": "repo for testing a gui using electron for the project generator",
  "license": "MIT",
  "main": "index.js",
  "repository": "github:openframeworks/projectGenerator",
  "author": {
    "name": "zach lieberman",
    "url": "https://openframeworks.cc"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "electron .",
    "start:prod": "electron .",
    "dist": "electron-builder",
    "dist:mac:x64": " electron-builder --mac --x64 ",
    "dist:mac:arm64": " electron-builder --mac --arm64",
    "dist:mac:universal": " electron-builder --mac --universal",
    "dist:win64": "electron-builder --win --x64",
    "dist:linux": "electron-builder --linux",
    "dist:linux64": "electron-builder --linux --x64",
    "dist:linux32": "electron-builder --linux --ia32"
  },
  "build": {
    "appId": "cc.openframeworks.projectGenerator.macos",
    "productName": "projectGenerator",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "static/icon/of.icns",
      "target": {
        "target": "dir",
        "arch": ["x64", "arm64"]
      },
      "mergeASARs": false,
      "x64ArchFiles": "Contents/Resources/app/app/projectGenerator"
    },
    "win": {
      "icon": "static/icon/of.ico"
    },
    "linux": {
      "icon": "static/icon"
    },
    "files": [
      "index.html",
      "app/**/*",
      "static/**/*",
      "index.js",
      "app.js",
      "preload.js",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json",
      {
          "from": "../dist",
          "filter": ["**/*"]
      }
    ],
    "asar": false
  },
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "moniker": "^0.1.2",
    "n-readlines": "^1.0.1",
    "@types/jquery": "^3.5.29"
    
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  }
}
